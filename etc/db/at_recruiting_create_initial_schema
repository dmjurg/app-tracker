CREATE TABLE `at_recruiting` (
   `id` int(10) NOT NULL AUTO_INCREMENT,
   `submission_date` date DEFAULT NULL,
   `status` int(11) DEFAULT NULL,
   `first_name` varchar(30) DEFAULT NULL,
   `last_name` varchar(30) DEFAULT NULL,
   `email` varchar(50) DEFAULT NULL,
   `phone_number` varchar(15) DEFAULT NULL,
   `street_address` varchar(50) DEFAULT NULL,
   `city` varchar(50) DEFAULT NULL,
   `state` varchar(2) DEFAULT NULL,
   `zipcode` varchar(10) DEFAULT NULL,
   `gpa_overall` decimal(4,3) DEFAULT NULL,
   `awards` varchar(200) DEFAULT NULL,
   `grad_date` date DEFAULT NULL,
   `referral` int(1) DEFAULT NULL,
   `referrer` varchar(50) DEFAULT NULL,
   `accommodation` int(1) DEFAULT NULL,
   `law` int(1) DEFAULT NULL,
   `start_date` date DEFAULT NULL,
   `other_langs` varchar(200) DEFAULT NULL,
   `position_id` int(5) DEFAULT NULL,
   `hr_viewed` tinyint(1) DEFAULT NULL,
   `github_url` varchar(200) DEFAULT NULL,
   `personal_url` varchar(200) DEFAULT NULL,
   `linkedin_url` varchar(200) DEFAULT NULL,
   `twitter` varchar(200) DEFAULT NULL,
   `university_id` int(11) DEFAULT NULL,
   `work_auth_id` int(11) DEFAULT NULL,
   PRIMARY KEY (`id`),
   KEY `fk_status` (`status`),
   KEY `fk_position_id` (`position_id`),
   KEY `university_id` (`university_id`),
   KEY `work_auth` (`work_auth_id`),
   CONSTRAINT `at_recruiting_ibfk_1` FOREIGN KEY (`university_id`) REFERENCES `at_lu_universities` (`id`),
   CONSTRAINT `fk_position_id` FOREIGN KEY (`position_id`) REFERENCES `at_lu_position` (`position_id`),
   CONSTRAINT `fk_status` FOREIGN KEY (`status`) REFERENCES `at_lu_candidate_status` (`status_id`)
 ) ENGINE=InnoDB AUTO_INCREMENT=74 DEFAULT CHARSET=latin1


